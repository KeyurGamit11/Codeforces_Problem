#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    cin>>t;
    while(t--){
	    int l,r;
	    cin>>l>>r;
	    int max_lcm = 2*l;
	    if(r<max_lcm){
	    	cout<<-1<<" "<<-1<<"\n";
	    }
	    else{
	    	cout<<l<<" "<<max_lcm<<"\n";
	    }
	}
    return 0;
}
