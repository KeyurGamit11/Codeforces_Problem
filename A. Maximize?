#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    cin>>t;
    while(t--){
    	int x;
    	cin>>x;
    	int y=1;
    	while(__gcd(x,y)+y < x){
    		y++;
    	}
    	cout<<y<<"\n";

    }
    return 0;
}
