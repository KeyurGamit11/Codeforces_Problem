#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    string s;
    cin>>s;
    string ans="hello";
    int i=0,j=0;
    bool ch=false;
    while(i<ans.size() && j<s.size()){
    	if(ans[i]==s[j]){
    		i++;
    		j++;
    	}
    	else{
    		j++;
    	}	
    }
    if(i==ans.size()){
    	cout<<"YES"<<"\n";
    }
    else{
    	cout<<"NO"<<"\n";
    }
    return 0;
}
