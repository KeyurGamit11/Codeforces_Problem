#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    ll t;
    cin>>t;
    while(t--){
    	ll n;
    	cin>>n;
    	if(n%3==1){ //follow 3k+1 sequance
    		cout<<"YES"<<"\n";
    	}
    	else{
    		cout<<"NO"<<"\n";
    	}
    }
    return 0;
}
