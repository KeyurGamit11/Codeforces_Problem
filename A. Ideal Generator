#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    cin>>t;
    while(t--){
    	int n;
    	cin>>n;
    	if(n%2==0){
    		cout<<"NO"<<"\n";
    	}
    	else{
    		cout<<"YES"<<"\n";
    	}
    }
    return 0;
}
