#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int a,b,c,d;
    cin>>a>>b>>c>>d;
    int total = 15;
    int sum = a+b+c+d;
    cout<<total-sum<<"\n";
    return 0;
}
