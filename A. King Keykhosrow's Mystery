#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    cin>>t;
    while(t--){
    	int a,b;
    	cin>>a>>b;
    	//just find lcm of both
    	cout<<a*b/__gcd(a,b)<<"\n";
    }
    return 0;
}
