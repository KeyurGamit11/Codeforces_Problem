#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    cin>>t;
    while(t--){
    	string s;
    	cin>>s;
    	int cnt=0;
    	for(auto i:s){
    		if(i=='1')cnt++;
    	}
    	cout<<cnt<<"\n";
    }
    return 0;
}
